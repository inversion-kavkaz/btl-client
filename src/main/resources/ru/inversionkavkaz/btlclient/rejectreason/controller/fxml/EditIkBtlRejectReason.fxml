<JInvLabel fx:id="lblREASON_SHORT" text="%REASON_SHORT" GridPane.columnIndex="0" GridPane.rowIndex="1" />
<JInvTextField fx:id="REASON_SHORT" fieldName="REASON_SHORT" prefColumnCount="50" GridPane.columnIndex="1" GridPane.rowIndex="1" />
<JInvLabel fx:id="lblREASON_FULL" text="%REASON_FULL" GridPane.columnIndex="0" GridPane.rowIndex="2" />
